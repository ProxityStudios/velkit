{
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true,
      "cache": false
    }
  }
}